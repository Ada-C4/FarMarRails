<div class="new-market-form">

<%= bootstrap_form_for @market, class: "form-group", url:({action: "#{@action}"}) do |f| %>
<%= f.label :name %>
<%= f.text_field :name, hide_label: true %>
<%= f.label :address %>
<%= f.text_field :address, hide_label: true  %>
<%= f.label :city %>
<%= f.text_field :city, hide_label: true  %>
<%= f.label :state %>
<%= f.text_field :state, hide_label: true  %>
<%= f.label :county %>
<%= f.text_field :county, hide_label: true  %>
<%= f.label :zip %>
<%= f.text_field :zip, hide_label: true  %>
<%= f.submit class: "submit", class: "btn btn-default" %>
<% end %>
</div>
