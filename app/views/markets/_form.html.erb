<%= form_for @market, :html => {:class => "form-inline"} do |f| %>
  <fieldset>
    <legend><%= legend_text %></legend>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :address %>
    <%= f.text_field :address %>
    <%= f.label :city %>
    <%= f.text_field :city %>
    <%= f.label :county %>
    <%= f.text_field :county %>
    <%= f.label :state %>
    <%= f.text_field :state %>
    <%= f.label :zip %>
    <%= f.text_field :zip %>
    <%= f.submit value: "#{button_text}", class: "new_button"%>
  </fieldset>
<% end %>
