<h1>Edit Yourself</h1>

<section>
  <%= form_for @market do |f| %>
    <fieldset>
      <legend>Edit Market</legend>
      <%= f.label :name %>
      <%= f.text_field :name %><br>
      <%= f.label :address %>
      <%= f.text_field :address %>
      <%= f.label :city %>
      <%= f.text_field :city %><br>
      <%= f.label :country %>
      <%= f.text_field :country %>
      <%= f.label :state %>
      <%= f.text_field :state %>
      <%= f.label :zip %>
      <%= f.text_field :zip %>
      <br>
      <%= f.submit %>
    </fieldset>
  <% end %>
</section>
