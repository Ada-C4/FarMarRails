<section>
<%= form_for @market do |f| %>
  <fieldset>
    <legend>New Market</legend>
    <div>
      <%= f.label :name, "name"%>
      <%= f.text_field :name %><br />
    </div>

    <div>
      <%= f.label :address, "address" %>
      <%= f.text_field :address %><br />
    </div>

    <div>
      <%= f.label :city, "city" %>
      <%= f.text_field :city %><br />
    </div>

    <div>
      <%= f.label :county, "county" %>
      <%= f.text_field :county %><br />
    </div>

    <div>
      <%= f.label :state, "state" %>
      <%= f.text_field :state %><br />
    </div>

    <div>
      <%= f.label :zip, "zip" %>
      <%= f.text_field :zip %><br />
    </div>

    <div>
      <%= f.submit %>
    </div>
  </fieldset>
<% end %>

<%= link_to "Back to Markets", markets_path %>

</section>
