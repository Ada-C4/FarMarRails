<div class="sub-header">
  <h3>You are acting as a market</h3>
  <%= link_to "Create new Market!", new_market_path %>
</div>

<table>
  <% @markets.each do |m| %>
    <tr>
      <td>
        <%= link_to "Edit", "/markets/#{m.id}/edit", method: :get %>
      </td>
      <td>
        <%= link_to m.name, market_path(m.id)  %>
      </td>
    </tr>
  <% end %>
</table>
