<div class="sub-header">
  <h3>
    <%= link_to "Create new Market!", new_market_path %>
  </h3>
</div>

<table>
  <% @markets.each do |m| %>
    <tr>
      <td>
        <%= link_to "Edit", edit_market_path(m.id), method: :get %>
      </td>
      <td>
        <%= link_to m.name, market_path(m.id)  %>
      </td>
    </tr>
  <% end %>
</table>
