<%=  form_for :sale, url: new_market_vendor_product_sale do |f| %>
<div class='legend'><h1><%= form_title %></h1></div>
  <fieldset>
    <%= f.label :name %>:
    <%= f.text_field :name %><br>
    <%= f.label :num_emps, 'Number of Employees' %>:
    <%= f.text_field :num_emps %><br>
    <%= hidden_field_tag :market_id, params[:market_id] %>
  </fieldset>
  <%= f.submit %>
<% end %>