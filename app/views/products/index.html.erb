<h2>Here are the products for <%= @vendor.name %>:</h2>
<div class="sign-in-button">
<%= button_to "Add new product", new_vendor_product_path, method: :get, class: "btn btn-default" %>
</div
<ul>
  <% @products.each do |product| %>
    <li>
      <%= product.name %> <%= link_to "Edit", edit_vendor_product_path(@vendor_id, product.id) %>
      <%= link_to "Delete", "/vendors/#{@vendor_id}/products/#{product.id}", method: :delete %>
    </li>
  <% end %>
</ul>
