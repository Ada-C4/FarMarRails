<div class="container">
  <h1>ALL OF THEM VENDORS</h1>
  <table class="table table-hover">
    <tr >
      <th>
        VENDOR
      </th>
      <th>
        MARKET
      </th>
      <th>
        STATE
      </th>
      <th>
        MORE INFO
      </th>
    </tr>
    <% @vendors.each do |vendor| %>
      <tr>
        <td>
          <%= vendor.name %>
        </td>
        <td>
          market
        </td>
       <td>
         state
       </td>
       <td>
         <%= link_to "Show Vendor", "/vendors/#{vendor.id}" %>
       </td>
     </tr>
    <% end %>
  </table>
</div>
