<section>
<%= form_for @new_market_vendor, :url => {:action => "create"} do |f| %>
  <fieldset>
    <legend>New Vendor</legend>
    <div>
      <%= f.label :name, "name"%>
      <%= f.text_field :name %><br />
    </div>

    <div>
      <%= f.label :num_employees, "number of employees" %>
      <%= f.text_field :num_employees %><br />
    </div>

    <div>
      <%= f.submit %>
    </div>
  </fieldset>
<% end %>

<%= link_to "Back to Market", market_path(@market) %>

</section>
