<%= form_for @vendor do |f| %>
  <fieldset>
    <legend>Vendor Information</legend>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :num_employees %>
    <%= f.text_field :num_employees %>

    <%= f.label :market_id %>
    <%= f.text_field :market_id %>
  </fieldset>
    <%= f.submit %>
<% end %>
