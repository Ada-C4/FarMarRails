<%=  form_for @market, :html => {class: 'market_form'} do |f| %>
<div class='legend'><h1 class='pg-header'><%= form_title %></h1></div>
  <fieldset>
    <%= f.label :name %>:<br>
    <%= f.text_field :name, class: 'text' %><br>
    <%= f.label :address %>:<br>
    <%= f.text_field :address, class: 'text' %><br>
    <%= f.label :city %>:<br>
    <%= f.text_field :city, class: 'text' %><br>
    <%= f.label :county %>:<br>
    <%= f.text_field :county, class: 'text' %><br>
    <%= f.label :state %>:<br>
    <%= f.text_field :state, class: 'text' %><br>
    <%= f.label :zip %>:<br>
    <%= f.text_field :zip, class: 'text' %><br>
  </fieldset>
  <%= f.submit 'Submit', class: 'form-button' %><br><br>
  <%= link_to 'all markets', markets_path, class: "top-nav-button" %>
<% end %>